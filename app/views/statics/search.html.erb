<h1>Results</h1>
<% if !@books.empty? %>
  <% @books.each do |book| %>
    <% if book.title != nil %>
      <h3><%= book.title %></h3>
    <% end %>
    <% if book.description != nil %>
      <p><%= book.description %></p>
    <% end %>
    <% if book.url != nil %>
      <img src="<%=book.url%>" alt=""  height="42" width="42">
    <% end %>
  <% end %>
<% end %>
